<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MyCQRS</title>

    <link rel="stylesheet" href="~/lib/semantic-ui/semantic.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div class="ui menu">
        <a class="item" asp-controller="Home" asp-action="Index">Home</a>
        <a class="item" asp-controller="Photographer" asp-action="Index">Photographers</a>
    </div>

    <div class="ui container">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2018 - MyCQRS</p>
        </footer>
    </div>

    <script src="~/lib/semantic-ui/semantic.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>